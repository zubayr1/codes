   "source": [
    "def solution(x, y):\n",
    "    if x==1 and y==1:\n",
    "        return 1\n",
    "    \n",
    "    if x>1:\n",
    "        return 1+ solution(x-1, y+1)\n",
    "    elif x==1:        \n",
    "        return 1+ solution(y-1, 1)"
   ]
